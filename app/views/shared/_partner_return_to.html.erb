<%- info = session[:referring_partner_info] %>
<%- if info %>
  <%- target_url = info[:return_url] || info[:url] %>
  <%- link_content = info[:name] %>
  <%- if info[:logo] %>
    <%- link_content = raw("<img src=\"#{info[:logo]}\" class=\"partner-logo\" alt=\"#{info[:name]}\">") %>
  <%- end %>

  <div class="partner-return-to">
    return to <%= raw("<a href=\"#{target_url}\">#{link_content}</a>") -%>
  </div>
<%- end %>
